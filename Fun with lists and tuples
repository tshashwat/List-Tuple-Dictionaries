l2=[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]
l1=[]
for a in range(len(l2)):
    l1.append(list(l2[a]))    
for i in range(len(l1)):
    for j in range(i,len(l1)):
        for k in range(1,len(l1[i])):
            if l1[i][k]>l1[j][k]:
                l1[i][k],l1[j][k]=l1[j][k],l1[i][k]

[tuple(a) for a in l1]   
